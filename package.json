{
  "name": "gwendoux_api",
  "version": "2.0.0",
  "description": "my personal website api",
  "main": "app.js",
  "repository": "git@github.com:gwendoux/gwendoux_api.git",
  "keywords": [
    "personal",
    "javascript",
    "css",
    "html"
  ],
  "author": "gwendoux",
  "license": "ISC",
  "engines": {
    "node": "4.3.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "posttest": "npm run lint",
    "unit-test": "NODE_ENV=test mocha test/test.js",
    "pretest": "rm -rf ./test/coverage",
    "test": "NODE_ENV=test istanbul cover _mocha test/test.js",
    "test:report": "open ./test/coverage/lcov-report/index.html",
    "prestart:watch": "npm run lint",
    "start": "node app.js",
    "start:watch": "nodemon",
    "start:dev": "nodejs-dashboard node app.js",
    "deploy:init": "pm2 ecosystem",
    "deploy:setup": "pm2 deploy ecosystem.json production setup",
    "deploy": "pm2 deploy ecosystem.json production",
    "postinstall": "npm prune"
  },
  "devDependencies": {
    "commander": "2.9.0",
    "convict": "2.0.0",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "3.17.1",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "must": "0.13.4",
    "nodemon": "1.11.0",
    "parallelshell": "2.0.0",
    "pm2": "2.4.2",
    "supertest": "3.0.0",
    "tracer": "0.8.7"
  },
  "dependencies": {
    "body-parser": "1.17.1",
    "express": "4.15.2",
    "instagram-node": "0.5.8",
    "loglevel": "1.4.1",
    "moment": "2.17.1",
    "mongoose": "4.9.0",
    "morgan": "1.8.1",
    "redis": "2.7.0",
    "request": "2.81.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
