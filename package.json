{
  "name": "gwendoux_api",
  "version": "1.7.0",
  "description": "my personal website api",
  "main": "app.js",
  "repository": "git@github.com:gwendoux/gwendoux_api.git",
  "keywords": [
    "personal",
    "javascript",
    "css",
    "html"
  ],
  "author": "gwendoux",
  "license": "ISC",
  "engines": {
    "node": "4.3.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "posttest": "npm run lint",
    "unit-test": "NODE_ENV=test mocha test/test.js",
    "pretest": "rm -rf ./test/coverage",
    "test": "NODE_ENV=test istanbul cover _mocha test/test.js",
    "test:report": "open ./test/coverage/lcov-report/index.html",
    "prestart:watch": "npm run lint",
    "start": "node app.js",
    "start:watch": "nodemon",
    "deploy:init": "pm2 ecosystem",
    "deploy:setup": "pm2 deploy ecosystem.json production setup",
    "deploy": "pm2 deploy ecosystem.json production",
    "postinstall": "npm prune",
    "safefreeze": "rm -rf node_modules npm-shrinkwrap.json && npm install && npm dedupe && npm test && npm shrinkwrap"
  },
  "devDependencies": {
    "commander": "2.9.0",
    "convict": "1.4.0",
    "eslint": "3.2.2",
    "istanbul": "0.4.4",
    "mocha": "3.0.2",
    "must": "0.13.2",
    "nodemon": "1.10.0",
    "parallelshell": "2.0.0",
    "pm2": "1.1.3",
    "supertest": "2.0.0",
    "supertest-as-promised": "4.0.0",
    "tracer": "0.8.3"
  },
  "dependencies": {
    "body-parser": "1.15.2",
    "escape-html": "1.0.3",
    "express": "4.14.0",
    "instagram-node": "0.5.8",
    "loglevel": "1.4.1",
    "moment": "^2.14.1",
    "morgan": "1.7.0",
    "parse-rss": "1.0.3",
    "redis": "2.6.2",
    "request": "2.74.0"
  }
}
